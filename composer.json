{
    "name": "vuravel/core",
    "description": "Vue.js & Laravel Core - The backend core",
    "keywords": [
        "vue","vuejs","vue-js",
        "laravel","php","framework","cms",
        "vuravel"
    ],
    "license": "MIT",
    "require": {
    	"php": ">=7.1.0",
        "laravel/framework": ">=5.6.0"
    },
    "autoload": {
        "psr-4": {
            "Vuravel\\Core\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vuravel\\Core\\VuravelCoreServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
